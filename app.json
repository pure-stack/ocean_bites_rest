{
  "expo": {
    "name": "ocean_bites_rest",
    "slug": "ocean_bites_rest",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "oceanbitesrest",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/logo.png",
      "backgroundColor": "#1F2024"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 124,
          "resizeMode": "contain",
          "backgroundColor": "#1F2024"
        }
      ],
      [
        "expo-asset",
        {
          "assets": [
            "./assets/pdf"
          ]
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Ocean Bites Rest to access your camera",
          "microphonePermission": "Allow Ocean Bites Rest to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ]
  }
}
